{
  "outline": {
    "splitCommand": "botright 50vs"
  },
  "diagnostic": {
    "errorSign": "",
    "warningSign": "",
    "infoSign": "",
    "hintSign": "",
    "messageTarget": "",
    "virtualText": true,
    "virtualTextCurrentLineOnly": false
  },
  "rust-analyzer": {
    "procMacro": {
      "enable": true
    },
    "updates": {
      "channel": "stable"
    },
    "rustfmt": {
      "enableRangeFormatting": true
    },
    "cargo": {
      "runBuildScripts": true
    }
  }
}
